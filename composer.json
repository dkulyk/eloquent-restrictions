{
    "name": "dkulyk/eloquent-restrictions",
    "description": "Laravel Eloquent query restrictions",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmytro Kulyk",
            "email": "lnkvisitor.ts@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/database": "^5.5 | ^6.0"
    },
    "autoload": {
        "psr-4": {
            "DKulyk\\Restrictions\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "DKulyk\\Restrictions\\RestrictionsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
